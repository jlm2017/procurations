extends layout.pug

block admin_content
  p.
    Total&nbsp;: #{total}
  nav.text-center(aria-label="Page navigation")
    ul.pagination
      - var page = 1
      while page < total/100 + 1
        li
          a(href='/admin/requests/' + page)= page++
  table.table
    tr
      th Email
      th Valide
      th Ville
    each elem in list
      tr
        td= elem.email
        td= elem.validation
        td= elem.insee
